- win_mapped_drive:
    letter: Z
    path: \\192.168.29.132\Patches
    state: present
  become: yes
  become_user: SYSTEM

- win_shell: |
    net use *
    Get-PSDrive -Name Z
  become: yes
  become_user: SYSTEM

- win_reboot:

- win_shell: |
    net use *
    Get-PSDrive -Name Z -ErrorAction Ignore
  become: yes
  become_user: SYSTEM
